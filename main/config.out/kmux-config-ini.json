{
 "container": {
  "svc": "on",
  "host": "on"
 },
 "config": {
  "general": {
   "vserver-path": "/var/lib/lxc/vserver",
   "arch": "i386",
   "stage": "/stage",
   "handle-deamons": "false"
  },
  "network": {
   "extern-network-mode": "dhcp",
   "extern-netip": "192.168.1",
   "netmask": "255.255.255.0",
   "extern-interface": "eth1",
   "extern-dns-fwd": "8.8.8.8"
  },
  "locales": {
   "lang": "de",
   "locale": "de_DE"
  },
  "fs": {
   "disk0": "/dev/sda",
   "raidlevel": 1
  }
 }
}